    "JS",
    "events",
    "jsevents",
    "custom",
    "events"
  ],
  "author": "hannad rehman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hannadrehman/JSEvents/issues"
  },
  "homepage": "https://github.com/hannadrehman/JSEvents#readme",
  "devDependencies": {
    "typescript": "^2.6.2"
  }
}
